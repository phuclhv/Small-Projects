<%= form_for(@student_hour_summary, remote: true) do |f| %>
  <%= f.label :month %>
  <%= f.text_field :month %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :monthly_total %>
  <%= f.text_field :monthly_total %>
  <%= f.label :to_date_total %>
  <%= f.text_field :to_date_total %>
  <%= f.label :remaining_required %>
  <%= f.text_field :remaining_required %>
  <%= f.label :previous_to_date_total %>
  <%= f.text_field :previous_to_date_total %>
  <%= f.label :activity_id %>
  <%= f.text_field :activity_id %>
  <%= f.label :school_user_id %>
  <%= f.text_field :school_user_id %>
  <%= f.submit "Save", class: "btn btn-lg btn-primary" %>
<% end %>