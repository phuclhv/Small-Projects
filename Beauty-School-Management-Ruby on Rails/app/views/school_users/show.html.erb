<% provide(:title, current_user.full_name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for current_user %>
        <%= current_user.full_name %>
      </h1>
    </section>
  </aside>
  <aside class="col-md-4">
    <section class="user_info">
      <%= current_school_user.ssn %>
    </section>
    
  </aside>
</div>

<% if !enrolled?(current_user) %>
  <%= render "program2" %>
<% end %>
